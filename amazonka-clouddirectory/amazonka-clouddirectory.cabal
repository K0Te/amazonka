          Test.AWS.CloudDirectory
        , Test.AWS.Gen.CloudDirectory
        , Test.AWS.CloudDirectory.Internal

    build-depends:
          amazonka-core == 1.6.1.*
        , amazonka-test == 1.6.1.*
        , amazonka-clouddirectory
        , base
        , bytestring
        , case-insensitive
